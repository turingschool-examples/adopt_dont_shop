<%= form_with url: path, method: method do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :breed %>
  <%= f.text_field :breed %>

  <%= f.label :age %>
  <%= f.number_field :age %>

  <%= f.label :adoptable %>
  <%= f.check_box :adoptable, value: true %>

  <%= f.submit %>
<% end %>