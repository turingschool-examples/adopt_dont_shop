<h1>New Application</h1>
<%= form_with url: "/applications", method: :post, local: true do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :state %>
    <%= f.text_field :state %>

    <%= f.label :zipcode %>
    <%= f.text_field :zip %>

    <%= f.label :reason %>
    <%= f.text_field :reason %>


    <%= f.submit%>

<% end %>
