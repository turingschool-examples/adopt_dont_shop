<%= form_with url: "/applications", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :street %>
  <%= form.text_field :street %>
  <%= form.label :city %>
  <%= form.text_field :city %>
  <%= form.label :state %>
  <%= form.text_field :state %>
  <%= form.label :zip %>
  <%= form.text_field :zip %>
  <%= form.label :description %>
  <%= form.text_field :description, :value => "I am a good person" %>
  <%= form.label :status %>
  <%= form.text_field :status, :value => "In Progress" %>
  <%= form.submit "submit" %>
<% end %>