<%= form_with url: "/applications/:id", method: :post, local: true do |form| %>
  <%= form.label :Name %>
  <%= form.text_field :Name %>
  <%= form.label :Street_Address %>
  <%= form.text_field :Street_Address %>
  <%= form.label :City %>
  <%= form.text_field :City %>
  <%= form.label :State %>
  <%= form.text_field :State %>
  <%= form.label :Zip_Code %>
  <%= form.text_field :Zip_Code %>
  <%= form.submit "submit" %>
<% end %>