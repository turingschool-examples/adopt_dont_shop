<%= form_with url: "/applications", method: :new, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :address %>
  <%= form.text_field :address %>

  <%= form.label :city %>
  <%= form.text_field :city %>

  <%= form.label :state %>
  <%= form.text_field :state %>

  <%= form.label :zip %>
  <%= form.number_field :zip %>

  <%= form.submit 'submit' %>
<% end %>
