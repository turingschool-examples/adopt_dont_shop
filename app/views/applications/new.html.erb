<h1>Create an Application</h1>
<ul>
<%= form_with url: "/applications", method: :post, local: true do |f| %>
  <li><%= f.label :name %></li>
  <li><%= f.text_field :name %></li>
  <li><%= f.label :street_address %></li>
  <li><%= f.text_field :street_address %></li>
  <li><%= f.label :city %></li>
  <li><%= f.text_field :city %></li>
  <li><%= f.label :state %></li>
  <li><%= f.text_field :state %></li>
  <li><%= f.label :zip %></li>
  <li><%= f.text_field :zip %></li>
  <li><%= f.label :description %></li>
  <li><%= f.text_field :description %></li>
  <li><%= f.submit 'Create Application'%></li>
<% end %>
</ul>